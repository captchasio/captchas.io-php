{
    "name": "CAPTCHAs.IO/CAPTCHAs.IO",
    "description": "PHP package for easy integration with CAPTCHAs.IO API",
    "keywords": [
        "CAPTCHAs.IO",
        "captcha",
        "recaptcha",
        "funcaptcha",
        "keycaptcha",
        "geetest",
        "anticaptcha",
        "hcaptcha",
        "recaptcha v2",
        "recaptcha v3",
        "bypass captcha",
        "solve captcha",
        "captcha solver",
        "decaptcha",
        "captchas.io"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "CAPTCHAs.IO",
            "email": "support@captchas.io",
            "homepage": "https://captchas.io"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*"
    },
    "autoload": {
        "psr-4": {
            "CaptchasIO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CaptchasIO\\Tests\\": "tests/"
        }
    }
}